{
  "name": "oza/user-images-manager",
  "description": "A package to help you manage your users images such as avatars or covers",
  "type": "library",
  "keywords": [
    "laravel",
    "Users",
    "Images"
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1"
  },
  "license": "MIT",
  "homepage": "https://github.com/oza75/UserImagesManager",
  "authors": [
    {
      "name": "Aboubacar Ouattara",
      "email": "abouba181@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Oza\\UserImagesManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Oza\\UserImagesManager\\Providers\\ManagerServiceProvider"
      ]
    }
  }
}
